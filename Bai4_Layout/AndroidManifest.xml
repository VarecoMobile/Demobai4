<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bai4"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".FrameActivity"
            android:label="FrameActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LinearActivity"
            android:label="LinearActivity" >
            <intent-filter>
                <action android:name="com.example.bai4.LinearActivity" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RelativeActivity"
            android:label="RelativeActivity" >
            <intent-filter>
                <action android:name="com.example.bai4.RelativeActivity" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TableActivity"
            android:label="TableActivity" >
            <intent-filter>
                <action android:name="com.example.bai4.TableActivity" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AbsoluteActivity"
            android:label="AbsoluteActivity" >
            <intent-filter>
                <action android:name="com.example.bai4.AbsoluteActivity" />
            </intent-filter>
        </activity>
    </application>

</manifest>